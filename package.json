		"preview": "vite preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@keenmate/js-common-helpers": "^0.2.1",
		"@keenmate/svelte-common-helpers": "^0.1.0",
