		"@sveltejs/package": "^2.0.2",
		"@types/inputmask": "^5.0.3",
		"admin-lte": "=3.0.5",
		"bootstrap": "^4.4.1",
		"CBuffer": "^2.2.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
