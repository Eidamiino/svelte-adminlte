		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.1",
		"svelte": "^3.58.0",
		"svelte-i18n": "^3.7.0",
		"svelte-preprocess": "^4.6.9",
		"svelte2tsx": "^0.5.13",
		"tippy.js": "^6.3.7",
